      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here
var newDate;
if (salesDate.indexOf("-") !== -1)
{
	var day = salesDate.substring(0, 2);
	if (day.substring(0,1).equals("0")){
		day = day.substring(1);
	}
	var month = salesDate.substring(3, 5);
	if (month.substring(0,1).equals("0")){
		month = month.substring(1);
	}
	newDate = month + "/" + day + "/" + salesDate.substring(6);
}
else
{
	newDate = salesDate;
}</jsScript_script>
      </jsScript>
    </jsScripts>
